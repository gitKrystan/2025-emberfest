{
  "name": "emberjs-todomvc",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for emberjs goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "vite",
    "test": "vite build --mode development && testem ci"
  },
  "imports": {
    "#app/*": "./app/*",
    "#config/*": "./app/config/*",
    "#components/*": "./app/components/*",
    "#env": "./app/config/environment.ts",
    "#routes/*": "./app/routes/*",
    "#services/*": "./app/services/*",
    "#types/*": "./app/types/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.1",
    "@ember-data/debug": "5.7.0",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.2.2",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/core": "^4.1.0",
    "@embroider/macros": "^1.18.0",
    "@embroider/router": "^3.0.1",
    "@embroider/vite": "^1.1.5",
    "@eslint/js": "^9.27.0",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "2.0.0-alpha.4",
    "@glint/environment-ember-loose": "2.0.0-alpha.3",
    "@glint/environment-ember-template-imports": "2.0.0-alpha.3",
    "@glint/template": "1.6.0-alpha.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "@warp-drive/core": "5.7.0",
    "@warp-drive/ember": "5.7.0",
    "@workspace/shared-data": "workspace:*",
    "@workspace/shared-utils": "workspace:*",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.1.2",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.6.0",
    "ember-cli-babel": "^8.2.0",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.6.0",
    "ember-template-lint": "^7.7.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-qunit": "^8.1.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.5",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^39.0.0",
    "testem": "^3.16.0",
    "todomvc-app-css": "^2.4.3",
    "todomvc-common": "^1.0.5",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": ">= 22",
    "npm": "use pnpm",
    "yarn": "use pnpm"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
